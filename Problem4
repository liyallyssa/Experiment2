#include<iostream>
	using namespace std;
	int main()
	{
	    int n1;
	    int n2;
	    int n3;
	    
	    cout<< "Enter first number: "<<endl;
	    cin>> n1;
	    cout<< "Enter second number: "<<endl;
	    cin>> n2;
	    cout<< "Enter third number: "<<endl;
	    cin>> n3;
	    
	    if(n1 >= n2 && n1 >= n3)
        cout << "The LARGEST of the three is " << n1;
        else if(n2 >= n1 && n2 >= n3) 
        cout << "The LARGEST of the three is " << n2;
        else if(n3 >= n1 && n3 >= n2)
        cout << "The LARGEST of the three is " << n3;
     
	    return 0;
		}
